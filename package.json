{
  "name": "sauce-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "watch": "vitest"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.3",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "c8": "^8.0.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.0.1",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.1.0"
  },
  "description": "Ralipo React Library",
  "main": "./dist/sauce-react.umd.js",
  "module": "./dist/sauce-react.es.js",
  "exports": {
    ".": {
      "import": "./dist/sauce-react.es.js",
      "require": "./dist/sauce-react.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unkletayo/sauce-react.git"
  },
  "keywords": [
    "ralipo",
    "react"
  ],
  "author": "Adetayo Akinsanya",
  "license": "MIT",
  "bugs": {
    "url": "git+https://github.com/unkletayo/sauce-react/issues"
  },
  "homepage": "git+https://github.com/unkletayo/sauce-react#readme"
}
